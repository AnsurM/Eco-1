<div class="col-xs-12">
    <div class="box">
        <div class="box-header">
            <h3 class="box-title">Employee Data Table</h3></br>

            <table class="table table-striped">
                <tbody>
                    <tr>
                        <th>
                            <a class="btn btn-xs btn-default" href="@Url.Action("NewHire","Account")">New User</a>
                        </th>
                        <th>
                            <a>Created</a>
                        </th>
                        <th></th>
                        <th>
                            Email/Login
                        </th>
                        <th>
                            <a>Display Name</a>
                        </th>
                        <th>
                            Role Name
                        </th>
                        <th>
                            <a>Last Name</a>
                        </th>
                    </tr>
                    @foreach (var s in Model)
            {
                <tr>
                    <th></th>
                    <th></th>
                    <th></th>
                    <th>@s.Email</th>
                    <th>@s.NormalizeUserName</th>
                    <th>@s.RoleName</th>
                </tr>
    }
                </tbody>
            </table>
        </div>
    </div>
</div>

