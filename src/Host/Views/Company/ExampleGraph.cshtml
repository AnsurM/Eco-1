            </tr>
            <tr>
                <th>Plums</th>
                <td>5</td>
                <td>11</td>
            </tr>
            <tr>
                <th>Bananas</th>
                <td>1</td>
                <td>1</td>
            </tr>
            <tr>
                <th>Oranges</th>
                <td>2</td>
                <td>4</td>
            </tr>
        </tbody>
    </table>
    <div id="container" style="min-width: 310px; height: 400px; margin: 0 auto"></div>


</div>
<script src="~/dist/js/BarChart.js"></script>
